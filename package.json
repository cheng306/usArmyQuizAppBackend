  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && next build",
    "start": "tsc && nodemon server/server.ts",
    "lint": "eslint . --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
