{
  "name": "usarmyquizappbackend",
  "version": "0.0.0",
  "license": "MIT",
  "engines": {
    "node": "12.0.0"
  },
  "scripts": {
    "build": "tsc && NODE_ENV=production nodemon src/server/server.ts",
    "start": "tsc && NODE_ENV=development nodemon src/server/server.ts",
    "lint": "eslint . --fix --max-warnings=0",
    "test": "NODE_ENV=development mocha -r ts-node/register test/**/*.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn test",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "mssql": "^6.3.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/chai": "^4.2.14",
    "@types/cors": "^2.8.9",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.9",
    "@types/mocha": "^8.2.0",
    "@types/mssql": "^6.0.7",
    "@types/node": "^14.14.16",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.11.0",
    "@typescript-eslint/parser": "^4.11.0",
    "chai": "^4.2.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^3.1.0",
    "lint-staged": "^10.5.3",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.6",
    "supertest": "^6.0.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  }
}
